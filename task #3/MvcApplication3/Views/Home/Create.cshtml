  @using (Html.BeginForm("Create", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    { 
      
<!doctype html>
<html>
<head>
     <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <title>Добавление пользователя</title>
</head>
      
<body ng-app="CreateUser">
 
   <h2>Добавление пользователя</h2>

  <div ng-controller="CreateUserController">

     <table>

    <tr><td><label>ФИО: </label></td><td><input type="text" id="Fio" required></td></tr>
    <tr><td><label>Логин: </label></td><td><input type="text" id="Login" required> </td></tr>
    <tr><td><label>Возраст: </label></td><td><input type="number" id="Age" required></td></tr>
    <tr><td><label>Телефон: </label></td><td><input type="text" id="Phone" required></td></tr>
    <tr><td><label>Фото: </label></td><td><input type="file" id="Foto" name ="file" required></td></tr>
                  
     </table>
    
     <div> <input type="submit"  value="Cоздать" ng-click="AddUser()"  /></div> 
    
     </div>

</body>

    <p style="font-size: 19px; position: static; ">
    @Html.ActionLink("Вернуться назад", "Admin")
    </p>

</html>
  }

<script type="text/javascript" src="~/Scripts/angular.min.js"></script>

<script type="text/javascript" src="@Url.Content("~/DatabaseUsersScripts/CreateUserScript.js")"></script>